{
  "name": "engagesphere-test-design-masterclass",
  "version": "1.0.0",
  "description": "Sample project with a Node.js backend and a React frontend.",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "start:frontend": "cd frontend && npm start",
    "start:server": "cd backend && npm start",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "api:run": "cypress run --tag 'API' --spec cypress/e2e/API/**.cy.js",
    "gui:run": "cypress run --tag 'GUI' --spec cypress/e2e/GUI/**.cy.js",
    "accessibility:run": "cypress run --tag 'A11' --spec cypress/e2e/AY11/**.cy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/EngageSphere-Test-Design-Masterclass-turma-2.git"
  },
  "keywords": [],
  "author": "Walmyr Filho <wlsf82@gmail.com> (https://walmyr.dev)",
  "license": "MIT",
  "homepage": "https://github.com/wlsf82/EngageSphere-Test-Design-Masterclass-turma-2/blob/main/README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axe-core": "^4.9.1",
    "cypress": "^13.13.0",
    "cypress-axe": "^1.5.0"
  },
  "dependencies": {
    "cypress-plugin-xhr-toggle": "^1.2.1"
  }
}
